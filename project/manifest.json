{
	"name": "Video", //程序名称 
	"version": "1.1", //版本号
	"description": "vip解析", //描述
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "VIP影视解析",
		"default_popup": "index.html" //是点击之后的弹窗操作
	},
	"permissions": [ //需要用到的权限
		"tabs",
		"http://*/*",
		"https://*/*",
		"notifications"
	],

	"manifest_version": 2 //一定要注意了，没有这个的话在最新版本的Chrome中无法安装插件
}
